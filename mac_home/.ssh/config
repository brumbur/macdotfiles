Host s5
    HostName 192.168.1.104
    Port 2222
    IdentityFile ~/.ssh/id_rs
	AddKeysToAgent yes

  Host github
    User brumbur
    HostName github.com
    Port 22
    IdentityFile ~/.ssh/id_ed25519
	AddKeysToAgent yes
    # UseKeychain only works on macOS - IgnoreUnknown directive should help
    IgnoreUnknown UseKeychain

  Host gitlab
    User brumbur
    HostName gitlab.com
    Port 22
    IdentityFile ~/.ssh/id_ed25519
	AddKeysToAgent yes
    # UseKeychain only works on macOS - IgnoreUnknown directive should help
    IgnoreUnknown UseKeychain

# just to test aliases. Instead of "ssh -T git@github.com" use with alias like this:
# > ssh -T ght
  Host ght github
    User git
    HostName github.com
    Port 22
    IdentityFile ~/.ssh/id_ed25519
	AddKeysToAgent yes
    # UseKeychain only works on macOS - IgnoreUnknown directive should help
    IgnoreUnknown UseKeychain    